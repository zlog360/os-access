{
  "rulesDirectory": [],
  "rules": {
    "class-name": true,
    "curly": false,
    "eofline": true,
    "indent": true,
    "max-line-length": [
      true, 
      140
    ],
    "no-arg": true,
    "no-construct": true,
    "no-duplicate-key": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-trailing-whitespace": true,
    "no-unused-expression": true,
    "no-unused-variable": true,
    "no-unreachable": true,
    "no-use-before-declare": true,
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else"
    ],
    "quotemark": [
      true, 
      "single"
    ],
    "semicolon": [
      false
    ],
    "trailing-comma": true,
    "triple-equals": true,
    "variable-name": false
  }
}
